     return main
 end
 return uitab
end


local placeId = game.PlaceId
if placeId == 2753915549 then
    OldWorld = true
elseif placeId == 4442272183 then
    NewWorld =  true
elseif placeId == 7449423635 then
    ThirdWorld =  true
end

local Library = Update:Window("NICE HUB","",Enum.KeyCode.RightControl);
Menu = Library:Tab("Menu")
AutoStats = Library:Tab("Stats")

local Time = Menu:Label("Executor Time");spawn(function() getgenv().Time = true;while true do wait(.1) UpdateTime() end end);function UpdateTime() local date = os.date("*t");local hour = (date.hour) % 24;local ampm = hour < 12 and "AM" or "PM";local timezone = string.format("%02i:%02i:%02i %s", ((hour -1) % 12) + 1, date.min, date.sec, ampm);local datetime = string.format("%02d/%02d/%04d", date.day, date.month, date.year);local LocalizationService = game:GetService("LocalizationService");local Players = game:GetService("Players");local player = Players.LocalPlayer;local name = player.Name;local result, code = pcall(function()   return LocalizationService:GetCountryRegionForPlayerAsync(player)  end);Time:Set(" : " .. timezone);Time:Set("Executor Time: " .. datetime .. " [ " .. code .. " ]");spawn(function() if getgenv().Time then pcall(function()  while wait() do  Time()  end end) end end) end


Menu:Seperator("Menu AutoFarm")




AutoStats:Toggle("Auto Stats Melee",false,function(value)
    _G.Melee = value
    while wait(.1) do
       if _G.Melee then
          pcall(function()
             local args = {
                [1] = "AddPoint",
                [2] = "Melee",
                [3] = Points
             }
             
             game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
          end)
       end
    end
 end)
 
 
